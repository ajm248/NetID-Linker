{
  "name": "Yale NetID Linker",
  "version": "1.2",
  "manifest_version": 3,
  "description": "Turns each netid into a link to the person's Yale Phonebook entry",
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "run_at": "document_end",
    "js": ["jquery-1.7.2.min.js", "netidlinker.js"],
    "all_frames": true
  }]


}
